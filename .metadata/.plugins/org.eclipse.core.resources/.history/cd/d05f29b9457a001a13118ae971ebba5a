package demo;

public class ExceptionHandlerClass {

	public static void main(String[] args) {

		try {
			System.out.println("Hello World  . . !\n");
			int i = 1/0;
			System.out.println("Completed");
		}catch(Exception e) {
			System.out.println("The Exception caught is : "+e);
			e.printStackTrace();
		}
		

	}

}
